project_name: generator
package: platform
quality: production
label: Matter CPMS Generator
description: > 
  Generates the key-pair, and CSR for the DAC
category: Example|Platform
filter:
  - name: "Device Type"
    value: ["SoC"]
  - name: "MCU"
    value: ["32-bit MCU"]
  - name: "Project Difficulty"
    value: ["Beginner"]
source:
  - path: app.cpp
  - path: config.cpp
  - path: credentials.cpp
  - path: commands.cpp
  - path: encoding.cpp
  - path: main.cpp
include:
  - path: .
    file_list:
    - path: app.h
    - path: config.h
    - path: credentials.h
    - path: commands.h
    - path: encoding.h
  - path: ../../examples/
    file_list:
    - path: lib/support/Base64.h
  - path: ../../src/
    file_list:
    - path: platform/silabs/SilabsConfig.h
component:
  - id: sl_system
  - id: device_init
  - id: cpp_support
  - id: iostream_rtt
  - id: nvm3_default
  - id: psa_its
  - id: psa_driver
  - id: psa_crypto
  - id: psa_crypto_ecc_secp256r1
  - id: psa_crypto_sha256
  - id: psa_crypto_ecdsa
  - id: mbedtls_pkcs5
  - id: mbedtls_ecc
  - id: mbedtls_x509
define:
  - name: "CUSTOM_TOKEN_HEADER"
    value: "custom-tokens.h"