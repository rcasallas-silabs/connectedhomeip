project_name: battery_service
package: wiseconnect
quality: alpha
label: BLE Battery Service
category: Example|WiSeConnect
description: >
  Demonstrates how to configure Battery Service GATT server with Battery service with notification characteristic UUID
filter:
  - name: "Wireless Technology"
    value: ["Bluetooth"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_ble_battery_service.c
include:
  - path: 
    file_list:
      - path: rsi_ble_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_ble
  - name: wiseconnect_sapi_bluetooth
toolchain_settings: []
readme:
  - path: readme.md
other_file:
  - path: resources/readme/batteryservicesoc.png
  - path: resources/readme/image2.png
  - path: resources/readme/batteryservicencp.png
  - path: resources/readme/image1.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
