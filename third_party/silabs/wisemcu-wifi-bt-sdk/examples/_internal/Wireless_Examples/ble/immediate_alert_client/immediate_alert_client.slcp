project_name: immediate_alert_client
package: wiseconnect
quality: alpha
label: BLE Immediate Alert Client
category: Example|WiSeConnect
description: >
  Demonstrates how a GATT client device accesses a GATT server device with  actual immediate alert notification functionality.
filter:
  - name: "Wireless Technology"
    value: ["Bluetooth"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_ble_immediate_alert_client.c
include:
  - path: 
    file_list:
      - path: rsi_ble_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_ble
  - name: wiseconnect_sapi_bluetooth
toolchain_settings: []
readme:
  - path: readme.md
other_file:
  - path: resources/readme/immediatealertncp.png
  - path: resources/readme/immediatealertsoc.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
