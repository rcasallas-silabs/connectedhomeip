project_name: firmware_update
package: wiseconnect
quality: alpha
label: Wi-Fi - Firmware Update via TCP Server
category: Example|WiSeConnect
description: >
  Wirelessly updates the RS911x firmware from a remote TCP server
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_firmware_update_app.c
include:
  - path: 
    file_list:
      - path: rsi_wlan_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_sapi_firmware_update
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
define:
  - name: EXECUTION_FROM_RAM
template_contribution:
  - name: EXECUTION_FROM_RAM
    value: 1
