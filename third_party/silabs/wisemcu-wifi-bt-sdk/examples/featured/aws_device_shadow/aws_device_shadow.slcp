project_name: aws_device_shadow
package: wiseconnect
quality: alpha
label: Wi-Fi - AWS IoT Device Shadow
category: Example|AWS IoT
description: >
  Creates an AWS IoT 'thing' that connects to the AWS IoT device shadow service using the MQTT protocol
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_aws_device_shadow.c
include:
  - path: 
    file_list:
      - path: aws_iot_config.h
      - path: rsi_wlan_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_resources_certificates
  - name: wiseconnect_third_party_aws
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
define:
  - name: FLOAT_PRINT_BYPASS
  - name: EXECUTION_FROM_RAM
template_contribution:
  - name: EXECUTION_FROM_RAM
    value: 1
toolchain_settings: []
