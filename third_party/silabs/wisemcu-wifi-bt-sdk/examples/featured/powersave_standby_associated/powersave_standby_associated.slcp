  - path: resources/readme/powersavestandbyassociatedapplicationconfiguration.png
  - path: resources/readme/image187.png
  - path: resources/readme/psassociateddebugprintswithdatatransfersoc.png
  - path: resources/readme/powersavestandbyassociatedsetupncp.png
  - path: resources/readme/psassociateddebugprintswithoutdatatransfersoc.png
  - path: resources/readme/powermeteravgcurrentconsumption.png
  - path: resources/readme/psassociateddebugprintswithoutdatatransferncp.png
  - path: resources/readme/powersavecurrentmeasurementpins.png
  - path: resources/readme/image185.png
  - path: resources/readme/enerygyprofilerpowersavestandbyassociated.png
  - path: resources/readme/powersavestandbyassociatedsetupsoc.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
