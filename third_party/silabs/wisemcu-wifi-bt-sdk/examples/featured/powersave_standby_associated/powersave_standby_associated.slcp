project_name: powersave_standby_associated
package: wiseconnect
quality: alpha
label: Wi-Fi - Powersave Standby Associated
category: Example|WiSeConnect
description: >
  Connects to a Wi-Fi Access Point in deep-sleep mode and intermittently wakes to send UDP packets
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_wlan_connected_sleep_app.c
include:
  - path: 
    file_list:
      - path: rsi_wlan_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
define:
  - name: TA_DEEP_SLEEP_COMMON_FLASH
toolchain_settings: []
readme:
  - path: readme.md
other_file:
