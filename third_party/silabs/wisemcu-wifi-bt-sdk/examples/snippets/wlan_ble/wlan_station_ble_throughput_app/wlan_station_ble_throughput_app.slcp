project_name: wlan_station_ble_throughput_app
package: wiseconnect
quality: alpha
label: WLAN Station BLE Throughput App
category: Example|WiSeConnect
description: >
  Demonstrates throughput measurement of Wi-Fi while BLE is in connection
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi+BLE"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.0
source:
  - path: rsi_ble_app.c
  - path: rsi_wlan_app.c
  - path: main.c
include:
  - path: 
    file_list:
      - path: rsi_wlan_config.h
      - path: rsi_bt_config.h
      - path: rsi_ble_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_ble
  - name: wiseconnect_sapi_bluetooth
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_resources_certificates
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
define:
   - name: FLOAT_PRINT_BYPASS
   - name: EXECUTION_FROM_RAM
template_contribution:
  - name: EXECUTION_FROM_RAM
    value: 1
