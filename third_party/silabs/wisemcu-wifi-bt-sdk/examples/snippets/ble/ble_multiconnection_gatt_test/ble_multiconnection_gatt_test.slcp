project_name: ble_multiconnection_gatt_test
package: wiseconnect
quality: alpha
label: BLE Multiple GATT Connections
category: Example|WiSeConnect
description: >
  Demonstrates how to connect a central device with multiple peripherals and vice versa.
filter:
  - name: "Wireless Technology"
    value: ["Bluetooth"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_ble_device_info.c
  - path: rsi_ble_main_task.c
  - path: rsi_ble_subtasks.c
  - path: main.c
include:
  - path: 
    file_list:
      - path: rsi_ble_device_info.h
      - path: rsi_common_config.h
      - path: rsi_ble_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_ble
  - name: wiseconnect_sapi_bluetooth
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
toolchain_settings: []
readme:
  - path: readme.md
other_file:
  - path: resources/readme/blecentralncp.png
  - path: resources/readme/blemultigattncp.png
  - path: resources/readme/blemultigattsoc.png
  - path: resources/readme/blemultigattmain1.png
  - path: resources/readme/blemultigattsecondary1.png
  - path: resources/readme/blemultigattconfigurations.png
  - path: resources/readme/blemultigattsecondary2.png
  - path: resources/readme/blemultigattmain2.png
  - path: resources/readme/blecentralsoc.png
  - path: resources/readme/blemultigattadvertiser.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
