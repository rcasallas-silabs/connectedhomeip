project_name: ble_datalength
package: wiseconnect
quality: alpha
label: BLE Data Length Extension
category: Example|WiSeConnect
description: >
  Demonstrates how to connect with a remote device in central role and initiates a data length extension.
filter:
  - name: "Wireless Technology"
    value: ["Bluetooth"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_ble_datalength.c
include:
  - path: 
    file_list:
      - path: rsi_ble_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_ble
  - name: wiseconnect_sapi_bluetooth
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
toolchain_settings: []
readme:
  - path: readme.md
other_file:
  - path: resources/readme/bledlapplicationconfigurations.png
  - path: resources/readme/bledlsocprints.png
  - path: resources/readme/bledlsoc.png
  - path: resources/readme/bledlncp.png
  - path: resources/readme/datalengthadvertiser.png
  - path: resources/readme/bledlncpprints.png
  - path: resources/readme/bledlconfigurations.png
  - path: resources/readme/bledledeviceconnected.png
  - path: resources/readme/bledlsocscanning.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
