project_name: https_client
package: wiseconnect
quality: alpha
label: HTTPS Client
category: Example|WiSeConnect
description: >
  Demonstrates how to create HTTPs client (secure) and do PUT, GET and POST operations with the server opened on remote peer
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_https_client_app.c
include:
  - path: 
    file_list:
      - path: index.txt
      - path: rsi_wlan_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_sapi_http_client
  - name: wiseconnect_resources_certificates
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
define:
  - name: FLOAT_PRINT_BYPASS
  - name: EXECUTION_FROM_RAM
template_contribution:
  - name: EXECUTION_FROM_RAM
    value: 1
