project_name: aws_iot_mqtt
package: wiseconnect
quality: alpha
label: AWS IoT MQTT Client
category: Example|WiSeConnect
description: >
  Create an AWS IoT 'thing' that interacts with AWS IoT using MQTT publish/subscribe
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_subscribe_publish_sample.c
include:
  - path: 
    file_list:
      - path: rsi_wlan_config.h
      - path: aws_iot_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_resources_certificates
  - name: wiseconnect_third_party_aws
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
define:
  - name: MQTT_YIELD_EN
  - name: EXECUTION_FROM_RAM
template_contribution:
  - name: EXECUTION_FROM_RAM
    value: 1
toolchain_settings: []
