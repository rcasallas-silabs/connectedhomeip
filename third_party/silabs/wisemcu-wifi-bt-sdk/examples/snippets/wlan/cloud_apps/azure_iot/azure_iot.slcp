project_name: azure_iot
package: wiseconnect
quality: alpha
label: Azure IoT Pub/Sub Client
category: Example|WiSeConnect
description: >
  Application connects to azure cloud.
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_azure_telemetry_c2d.c
include:
  - path: 
    file_list:
      - path: azure_iot_config.h
      - path: rsi_wlan_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_resources_certificates
  - name: wiseconnect_third_party_azure
define:
  - name: NO_LOGGING
  - name: AZURE_ENABLE
  - name: RSI_DEBUG_PRINTS
  - name: EXECUTION_FROM_RAM
  - name: __error_t_defined
template_contribution:
