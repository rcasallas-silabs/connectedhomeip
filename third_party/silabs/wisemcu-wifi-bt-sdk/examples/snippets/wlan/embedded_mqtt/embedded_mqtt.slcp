project_name: embedded_mqtt
package: wiseconnect
quality: alpha
label: Embedded MQTT
category: Example|WiSeConnect
description: >
  Demonstrates how to build an MQTT client and connects with an MQTT broker.
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_emb_mqtt.c
include:
  - path: 
    file_list:
      - path: rsi_wlan_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_sapi_mqtt_client
  - name: wiseconnect_third_party_mqtt_client
  - name: wiseconnect_sapi_freertos
  - name: wiseconnect_third_party_freertos
toolchain_settings: []
readme:
  - path: readme.md
other_file:
  - path: resources/readme/embeddedmqttclientwlanconfig.png
  - path: resources/readme/mqttexploreraddtopic.png
  - path: resources/readme/embeddedmqttclientsetupsoc.png
  - path: resources/readme/mosquittopublishfrommodule.png
  - path: resources/readme/embeddedmqttapplicationconfiguration.png
  - path: resources/readme/mosquittoconnectsubscribe.png
  - path: resources/readme/mqttexplorercreateconnection.png
  - path: resources/readme/mosquittoporterrorresolution.png
  - path: resources/readme/embeddedmqttclientoutputprintsncp.png
  - path: resources/readme/mqttexploreradvancedoption.png
  - path: resources/readme/embeddedmqttclientsetupncp.png
  - path: resources/readme/mqttexplorerdeleteconnections.png
  - path: resources/readme/mqttexplorermsg.png
  - path: resources/readme/mqttexplorerdeletetopics.png
  - path: resources/readme/mqttexplorerconnect.png
  - path: resources/readme/mqttserver.png
  - path: resources/readme/embeddedmqttclientoutputprintssoc.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
