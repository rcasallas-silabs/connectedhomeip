project_name: http_client_v6
package: wiseconnect
quality: alpha
label: HTTP Client_v6
category: Example|WiSeConnect
description: >
  Demonstrates how to create HTTP client and do PUT, GET and POST operations with the server opened on remote peer
filter:
  - name: "Wireless Technology"
    value: ["Wi-Fi"]
  - name: "Device Type"
    value: Undefined
  - name: "Project Difficulty"
    value: ["Advanced"]
xml_filter: Undefined
sdk: {id: gecko_sdk, version: 4.1.0}
sdk_extension:
  - id: wiseconnect
    version: 1.0.1
source:
  - path: rsi_http_client_app.c
include:
  - path: 
    file_list:
      - path: index.txt
      - path: rsi_wlan_config.h
requires:
  - name: wiseconnect_platform
  - name: wiseconnect_sapi_wlan
  - name: wiseconnect_sapi_sockets
  - name: wiseconnect_sapi_http_client
  - name: wiseconnect_resources_certificates
  - name: wiseconnect_sapi_ipv6_utils
define:
  - name: EXECUTION_FROM_RAM
template_contribution:
  - name: EXECUTION_FROM_RAM
    value: 1
toolchain_settings: []
readme:
  - path: readme.md
other_file:
  - path: resources/readme/httpclientv6cmdsuccess.png
  - path: resources/readme/httpclientv6applicationprintssoc.png
  - path: resources/readme/httpclientv6applicationconfiguration.png
  - path: resources/readme/httpclientv6setupncp.png
  - path: resources/readme/httpclientv6applicationprintsncp.png
  - path: resources/readme/httpclientv6setupsoc.png
  - path: resources/readme/httpclientv6serverstartcommand.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
